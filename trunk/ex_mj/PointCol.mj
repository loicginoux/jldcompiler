import Point;

class PointCol extends Point {
  private int col;

  public  PointCol (int xi,int yi, int ci) {
  		super = new Point(xi, yi);
		col = ci;
	}

  public void changer(int val){
    this.col = val;
    }
  
  public void init (int xi,int yi) {
    this.x = xi;
    this.y = yi;
    this.col = 84;
    }

  public int type () {
    return gety();
    } 
  
  }

